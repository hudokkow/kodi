<changelog xmlns="http://www.codesynthesis.com/xmlns/odb/changelog" database="sqlite" version="1">
  <model version="1">
    <table name="CODBPerson" kind="object">
      <column name="idPerson" type="INTEGER" null="false"/>
      <column name="name" type="TEXT" null="false"/>
      <column name="art" type="INTEGER" null="true"/>
      <primary-key auto="true">
        <column name="idPerson"/>
      </primary-key>
      <foreign-key name="art_fk" deferrable="DEFERRED">
        <column name="art"/>
        <references table="CODBArt">
          <column name="idArt"/>
        </references>
      </foreign-key>
    </table>
  </model>
</changelog>
